{
  "private": true,
  "type": "module",
  "scripts": {
    "format": "prettier --log-level warn -w .",
    "test": "vitest run && tsc",
    "build": "esbuild server.ts --outfile=server.mjs --platform=node",
    "dev": "pnpm run dev:deno",
    "dev:bun": "bun server.ts",
    "dev:deno": "deno run --allow-read --allow-net server.ts",
    "dev:node": "pnpm run build && node server.mjs"
  },
  "devDependencies": {
    "@commander-js/extra-typings": "^11.0.0",
    "@types/node": "^20.6.3",
    "bun": "^1.0.2",
    "commander": "^11.0.0",
    "deno-bin": "^1.37.0",
    "esbuild": "^0.19.3",
    "prettier": "^3.0.3",
    "tslib": "^2.6.2",
    "typescript": "^5.2.2",
    "vitest": "^0.34.4"
  },
  "dependencies": {
    "client-zip": "^2.4.4",
    "valibot": "^0.17.0"
  }
}
