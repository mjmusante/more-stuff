{
  "name": "gosub",
  "version": "0.0.1",
  "private": true,
  "type": "module",
  "scripts": {
    "hooks": "rm -f .git/hooks/pre-commit && ln -s ../../.githooks/pre-commit .git/hooks/pre-commit",
    "format": "prettier --plugin prettier-plugin-svelte --log-level warn -w .",
    "dev": "vite dev",
    "build": "vite build",
    "preview": "vite preview",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json"
  },
  "devDependencies": {
    "@sveltejs/adapter-auto": "^2.0.0",
    "@sveltejs/kit": "^1.20.4",
    "prettier-plugin-svelte": "^3.0.3",
    "prettier": "^3.0.3",
    "svelte-check": "^3.4.3",
    "svelte": "^4.0.5",
    "tslib": "^2.4.1",
    "typescript": "^5.0.0",
    "vite": "^4.4.2"
  }
}
