{
  "name": "gosub-goproxy",
  "version": "0.0.10",
  "description": "GOPROXY implementation that allows Go modules to live in subdirectories",
  "keywords": [
    "golang",
    "goproxy",
    "gosub"
  ],
  "license": "0BSD",
  "author": "Iain Merrick <iain@moreplease.com>",
  "repository": {
    "type": "git",
    "url": "https://github.com/more-please/gosub-goproxy.git",
    "directory": "src"
  },
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "type": "module",
  "files": [
    "README.md",
    "LICENSE",
    "dist/goproxy.d.ts",
    "dist/gosub.d.ts",
    "dist/index.d.ts",
    "dist/index.js"
  ],
  "scripts": {
    "format": "prettier --log-level warn -w .",
    "test": "tsc && ts-browser-compat",
    "build": "esbuild index.ts --outdir=dist --format=esm --bundle --minify",
    "prepare": "tsc && pnpm run build && cp -f ../README.md ../LICENSE ."
  },
  "devDependencies": {
    "@journeyapps/ts-browser-compat": "^0.1.2",
    "esbuild": "^0.19.3",
    "prettier": "^3.0.3",
    "typescript": "^5.2.2"
  },
  "dependencies": {
    "client-zip": "^2.4.4",
    "valibot": "^0.17.1"
  }
}
