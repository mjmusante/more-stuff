{
  "$schema": "https://turbo.build/schema.json",
  "tasks": {
    "//#codegen": {
      "dependsOn": []
    },
    "format": {
      "dependsOn": []
    },
    "build": {
      "dependsOn": [
        "^build",
        "^prepare",
        "format",
        "//#codegen"
      ],
      "outputs": [
        "build/**"
      ]
    },
    "test": {
      "dependsOn": [
        "^test",
        "build"
      ]
    },
    "prepare": {
      "dependsOn": [
        "^prepare",
        "build",
        "test"
      ],
      "outputs": [
        "dist/**"
      ]
    },
    "push": {
      "inputs": [
        "build/**"
      ],
      "dependsOn": [
        "^push",
        "test"
      ]
    }
  }
}
